@prefix : <http://www.semanticweb.org/bimmer/ob#> .
@prefix dc: <http://purl.org/dc/elements/1.1> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://www.semanticweb.org/bimmer/ob#> .

<http://www.semanticweb.org/bimmer/ob#> rdf:type owl:Ontology ;
                                         dcterms:creator <http://www.semanticweb.org/bimmer/ob> ;
                                         dcterms:license ""@en ;
                                         rdfs:comment "This ontology aims to model the Occupancy Behavior domain according to the obXML schema (https://behavior.lbl.gov/?q=obXML)"@en ;
                                         owl:versionInfo "0.0.1" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/publisher
<http://purl.org/dc/elements/1.1/publisher> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
dcterms:modified rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/bimmer/ob#definedByEnvelop
:definedByEnvelop rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :ThermalNeed ;
                  rdfs:range :ThermalComfortEnvelop .


###  http://www.semanticweb.org/bimmer/ob#definedByRange
:definedByRange rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:range :ParameterRange .


###  http://www.semanticweb.org/bimmer/ob#detailedBy
:detailedBy rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty .


###  http://www.semanticweb.org/bimmer/ob#drivenBy
:drivenBy rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/bimmer/ob#hasBehavior
:hasBehavior rdf:type owl:ObjectProperty ;
             rdfs:domain :Occupant ;
             rdfs:range :Behavior .


###  http://www.semanticweb.org/bimmer/ob#hasBuildingType
:hasBuildingType rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/bimmer/ob#hasCoefficient
:hasCoefficient rdf:type owl:ObjectProperty ;
                rdfs:domain :InteractionFormula .


###  http://www.semanticweb.org/bimmer/ob#hasDayOfWeek
:hasDayOfWeek rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :DriverTime ;
              rdfs:range time:DayOfWeek .


###  http://www.semanticweb.org/bimmer/ob#hasEnvironmentParameter
:hasEnvironmentParameter rdf:type owl:ObjectProperty ;
                         rdfs:domain :Environment ;
                         rdfs:range :EnvironmentParameter .


###  http://www.semanticweb.org/bimmer/ob#hasIndependentVariable
:hasIndependentVariable rdf:type owl:ObjectProperty ;
                        rdfs:domain :InteractionFormula .


###  http://www.semanticweb.org/bimmer/ob#hasMeeting
:hasMeeting rdf:type owl:ObjectProperty ;
            rdfs:domain :Space ;
            rdfs:range :Meeting .


###  http://www.semanticweb.org/bimmer/ob#hasOccupant
:hasOccupant rdf:type owl:ObjectProperty ;
             rdfs:domain :Space ;
             rdfs:range :Occupant .


###  http://www.semanticweb.org/bimmer/ob#hasSeason
:hasSeason rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :DriverTime ;
           rdfs:range :Season .


###  http://www.semanticweb.org/bimmer/ob#hasSpace
:hasSpace rdf:type owl:ObjectProperty ;
          rdfs:domain :Building ;
          rdfs:range :Space .


###  http://www.semanticweb.org/bimmer/ob#hasSpaceType
:hasSpaceType rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/bimmer/ob#hasSystem
:hasSystem rdf:type owl:ObjectProperty ;
           rdfs:domain :Space ;
           rdfs:range :System .


###  http://www.semanticweb.org/bimmer/ob#hasTimeOfDay
:hasTimeOfDay rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :DriverTime ;
              rdfs:range :TimeOfDay .


###  http://www.semanticweb.org/bimmer/ob#influencedByEnvironment
:influencedByEnvironment rdf:type owl:ObjectProperty ;
                         rdfs:domain :Driver ;
                         rdfs:range :Environment .


###  http://www.semanticweb.org/bimmer/ob#influencedByHabit
:influencedByHabit rdf:type owl:ObjectProperty ;
                   rdfs:domain :Driver ;
                   rdfs:range :Habit .


###  http://www.semanticweb.org/bimmer/ob#influencedByNeed
:influencedByNeed rdf:type owl:ObjectProperty ;
                  rdfs:range :Need .


###  http://www.semanticweb.org/bimmer/ob#interactsWith
:interactsWith rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/bimmer/ob#leadsTo
:leadsTo rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/bimmer/ob#modeledBy
:modeledBy rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty .


###  http://www.semanticweb.org/bimmer/ob#occursAt
:occursAt rdf:type owl:ObjectProperty ;
          rdfs:domain :Driver ;
          rdfs:range :DriverTime .


###  http://www.semanticweb.org/bimmer/ob#occursIn
:occursIn rdf:type owl:ObjectProperty ;
          rdfs:domain :Driver ;
          rdfs:range :Space .


###  http://www.semanticweb.org/bimmer/ob#operationMode
:operationMode rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :System .


###  http://www.semanticweb.org/bimmer/ob#programmedAt
:programmedAt rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:range :Schedule .


###  http://www.semanticweb.org/bimmer/ob#relatedTo
:relatedTo rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :ParameterRange ;
           rdfs:range :EnvironmentParameter .


###  http://www.semanticweb.org/bimmer/ob#specifiedBy
:specifiedBy rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty .


###  http://www.semanticweb.org/bimmer/ob#triggeredBy
:triggeredBy rdf:type owl:ObjectProperty ;
             rdfs:domain :Driver ;
             rdfs:range :DriverEvent .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/bimmer/ob#buildingAddress
:buildingAddress rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Building ;
                 rdfs:range xsd:string .


###  http://www.semanticweb.org/bimmer/ob#coefficientName
:coefficientName rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :CoefficientSchema ;
                 rdfs:range xsd:string .


###  http://www.semanticweb.org/bimmer/ob#coefficientValue
:coefficientValue rdf:type owl:DatatypeProperty ;
                  rdfs:domain :CoefficientSchema ;
                  rdfs:range xsd:float .


###  http://www.semanticweb.org/bimmer/ob#controlValue
:controlValue rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/bimmer/ob#description
:description rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/bimmer/ob#duration
:duration rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:range xsd:integer .


###  http://www.semanticweb.org/bimmer/ob#endTime
:endTime rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Schedule ;
         rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/bimmer/ob#identifier
:identifier rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty .


###  http://www.semanticweb.org/bimmer/ob#maxNumberOccupants
:maxNumberOccupants rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :Space ;
                    rdfs:range xsd:integer .


###  http://www.semanticweb.org/bimmer/ob#maxRangeValue
:maxRangeValue rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :ParameterRange ;
               rdfs:range xsd:float .


###  http://www.semanticweb.org/bimmer/ob#meetingProbability
:meetingProbability rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :Meeting ;
                    rdfs:range xsd:float .


###  http://www.semanticweb.org/bimmer/ob#minNumberOccupants
:minNumberOccupants rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :Space ;
                    rdfs:range xsd:integer .


###  http://www.semanticweb.org/bimmer/ob#minRangeValue
:minRangeValue rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :ParameterRange ;
               rdfs:range xsd:float .


###  http://www.semanticweb.org/bimmer/ob#occupantAge
:occupantAge rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Occupant ;
             rdfs:range xsd:integer .


###  http://www.semanticweb.org/bimmer/ob#occupantGender
:occupantGender rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Occupant ;
                rdfs:range xsd:string .


###  http://www.semanticweb.org/bimmer/ob#occupantJobtype
:occupantJobtype rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Occupant ;
                 rdfs:range xsd:string .


###  http://www.semanticweb.org/bimmer/ob#occupantLifestyle
:occupantLifestyle rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Occupant ;
                   rdfs:range xsd:string .


###  http://www.semanticweb.org/bimmer/ob#occupantName
:occupantName rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Occupant ;
              rdfs:range xsd:string .


###  http://www.semanticweb.org/bimmer/ob#parameterName
:parameterName rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :EnvironmentParameter ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/bimmer/ob#parameterUnit
:parameterUnit rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :EnvironmentParameter ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/bimmer/ob#percentTimePresence
:percentTimePresence rdf:type owl:DatatypeProperty ;
                     rdfs:domain :SpaceOccupancySpecification ;
                     rdfs:range xsd:float .


###  http://www.semanticweb.org/bimmer/ob#startTime
:startTime rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Schedule ;
           rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/bimmer/ob#typicalTime
:typicalTime rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :EventSpecification .


###  http://www.w3.org/2002/07/owl#topDataProperty
owl:topDataProperty rdfs:range xsd:float .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/bimmer/ob#AccousticNeed
:AccousticNeed rdf:type owl:Class ;
               rdfs:subClassOf :PhysicalNeed ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :definedByRange ;
                                 owl:someValuesFrom :ParameterRange
                               ] ;
               rdfs:label "Accoustic Need"@en .


###  http://www.semanticweb.org/bimmer/ob#Action
:Action rdf:type owl:Class ;
        rdfs:comment "Actions are the interactions with systems or activities that occupants can perform to achieve environmental comfort."@en ;
        rdfs:label "Action"@en .


###  http://www.semanticweb.org/bimmer/ob#Behavior
:Behavior rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :leadsTo ;
                            owl:someValuesFrom :Action
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :drivenBy ;
                            owl:allValuesFrom :Driver
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :influencedByNeed ;
                            owl:allValuesFrom :Need
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :interactsWith ;
                            owl:allValuesFrom :System
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :identifier ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :description ;
                            owl:allValuesFrom xsd:string
                          ] ;
          rdfs:comment "Class that represent the behaviour of the occupant."@en ;
          rdfs:label "Behavior"@en .


###  http://www.semanticweb.org/bimmer/ob#Building
:Building rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasBuildingType ;
                            owl:someValuesFrom :BuildingType
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasSpace ;
                            owl:someValuesFrom :Space
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasSpace ;
                            owl:allValuesFrom :Space
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :identifier ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :buildingAddress ;
                            owl:allValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :description ;
                            owl:allValuesFrom xsd:string
                          ] ;
          rdfs:comment "Building where the occupancy behaviors occur."@en ;
          rdfs:label "Building"@en .


###  http://www.semanticweb.org/bimmer/ob#BuildingType
:BuildingType rdf:type owl:Class .


###  http://www.semanticweb.org/bimmer/ob#CoefficientSchema
:CoefficientSchema rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty :coefficientName ;
                                     owl:someValuesFrom xsd:string
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :coefficientValue ;
                                     owl:someValuesFrom xsd:float
                                   ] ;
                   rdfs:comment "N-ary class to represent the value and the names of coefficients inside mathematical formulas."@en ;
                   rdfs:label "Coefficient Schema"@en .


###  http://www.semanticweb.org/bimmer/ob#ConstantValueFormula
:ConstantValueFormula rdf:type owl:Class ;
                      rdfs:subClassOf :InteractionFormula ;
                      rdfs:comment "Class to represent mathematical formulas of the type Constant Value."@en ;
                      rdfs:label "Constant Value Formula"@en .


###  http://www.semanticweb.org/bimmer/ob#ControlType
:ControlType rdf:type owl:Class .


###  http://www.semanticweb.org/bimmer/ob#DaylightFactorParameter
:DaylightFactorParameter rdf:type owl:Class ;
                         rdfs:subClassOf :EnvironmentParameter ;
                         rdfs:comment "Parameter to represent the indoor daylight."@en ;
                         rdfs:label "Daylight Factor Parameter"@en .


###  http://www.semanticweb.org/bimmer/ob#Driver
:Driver rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :influencedByEnvironment ;
                          owl:allValuesFrom :Environment
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :occursAt ;
                          owl:allValuesFrom :DriverTime
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :occursIn ;
                          owl:allValuesFrom :Space
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :triggeredBy ;
                          owl:allValuesFrom :DriverEvent
                        ] ;
        rdfs:comment "Drivers represent the environmental factors that stimulate occupants to fulfill a physical, physiological or psychological need."@en ;
        rdfs:label "Driver"@en .


###  http://www.semanticweb.org/bimmer/ob#DriverEvent
:DriverEvent rdf:type owl:Class ;
             rdfs:label "Driver Event"@en .


###  http://www.semanticweb.org/bimmer/ob#DriverTime
:DriverTime rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasDayOfWeek ;
                              owl:allValuesFrom time:DayOfWeek
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasSeason ;
                              owl:allValuesFrom :Season
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasTimeOfDay ;
                              owl:allValuesFrom :TimeOfDay
                            ] ;
            rdfs:comment "N-ary class to define the time, day and season when a specific driver occurs."@en ;
            rdfs:label "Driver Time"@en .


###  http://www.semanticweb.org/bimmer/ob#Environment
:Environment rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasEnvironmentParameter ;
                               owl:someValuesFrom :EnvironmentParameter
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasEnvironmentParameter ;
                               owl:allValuesFrom :EnvironmentParameter
                             ] ;
             rdfs:comment "Class to represent the environmental physical conditions that could be causing the occupant behavior."@en ;
             rdfs:label "Environment"@en .


###  http://www.semanticweb.org/bimmer/ob#EnvironmentParameter
:EnvironmentParameter rdf:type owl:Class ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty :identifier ;
                                        owl:someValuesFrom xsd:string
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :parameterUnit ;
                                        owl:someValuesFrom xsd:string
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :description ;
                                        owl:allValuesFrom xsd:string
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :parameterName ;
                                        owl:allValuesFrom xsd:string
                                      ] ;
                      rdfs:comment "Class that represent a physical variable of the environment where the driver occurs. It could represent variables that impact directly on the comfort levels of the occupant."@en ;
                      rdfs:label "Environment Parameter"@en .


###  http://www.semanticweb.org/bimmer/ob#Equipment
:Equipment rdf:type owl:Class ;
           rdfs:subClassOf :System ;
           rdfs:comment "Class to represent equipments the occupant interact with but do not influence the environment. These systems also contribute in the energy performance analysis." ;
           rdfs:label "Equipment"@en .


###  http://www.semanticweb.org/bimmer/ob#EventSpecification
:EventSpecification rdf:type owl:Class ;
                    rdfs:subClassOf :MovementSpecification ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :detailedBy ;
                                      owl:someValuesFrom :DriverEvent
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :programmedAt ;
                                      owl:someValuesFrom :Schedule
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :typicalTime ;
                                      owl:someValuesFrom xsd:dateTime
                                    ] ;
                    owl:disjointWith :SpaceOccupancySpecification ;
                    rdfs:label "Event Specification"@en .


###  http://www.semanticweb.org/bimmer/ob#GlareParameter
:GlareParameter rdf:type owl:Class ;
                rdfs:subClassOf :EnvironmentParameter ;
                rdfs:comment "Parameter to represent the glare inside a space."@en ;
                rdfs:label "Glare Parameter"@en .


###  http://www.semanticweb.org/bimmer/ob#HVAC
:HVAC rdf:type owl:Class ;
      rdfs:subClassOf :System ;
      rdfs:label "HVAC"@en .


###  http://www.semanticweb.org/bimmer/ob#Habit
:Habit rdf:type owl:Class ;
       rdfs:comment "Class to represent occupant habits."@en ;
       rdfs:label "Habit"@en .


###  http://www.semanticweb.org/bimmer/ob#IAQNeed
:IAQNeed rdf:type owl:Class ;
         rdfs:subClassOf :PhysicalNeed ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :definedByRange ;
                           owl:someValuesFrom :ParameterRange
                         ] ;
         rdfs:label "IAQ Need"@en .


###  http://www.semanticweb.org/bimmer/ob#IAQParameter
:IAQParameter rdf:type owl:Class ;
              rdfs:subClassOf :EnvironmentParameter ;
              rdfs:comment "Parameter to represent the indoor air quality."@en ;
              rdfs:label "IAQ Parameter"@en .


###  http://www.semanticweb.org/bimmer/ob#IlluminanceParameter
:IlluminanceParameter rdf:type owl:Class ;
                      rdfs:subClassOf :EnvironmentParameter ;
                      rdfs:comment "Parameter to represent the amount of illumination inside a space."@en ;
                      rdfs:label "Illuminance Parameter"@en .


###  http://www.semanticweb.org/bimmer/ob#Inaction
:Inaction rdf:type owl:Class ;
          rdfs:subClassOf :Action ;
          rdfs:comment "The Inaction class represents the decision of an occupant to not act and remain uncomfortable within a space."@en ;
          rdfs:label "Inaction"@en .


###  http://www.semanticweb.org/bimmer/ob#Interaction
:Interaction rdf:type owl:Class ;
             rdfs:subClassOf :Action ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :modeledBy ;
                               owl:someValuesFrom :InteractionFormula
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :controlValue ;
                               owl:someValuesFrom xsd:float
                             ] ;
             rdfs:comment "The Interaction class represents the interaction of the occupant with the systems within the space to recover its comfort."@en ;
             rdfs:label "Interaction"@en .


###  http://www.semanticweb.org/bimmer/ob#InteractionFormula
:InteractionFormula rdf:type owl:Class ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasCoefficient ;
                                      owl:someValuesFrom :CoefficientSchema
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasIndependentVariable ;
                                      owl:someValuesFrom :EnvironmentParameter
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :description ;
                                      owl:someValuesFrom xsd:string
                                    ] ;
                    rdfs:comment "Formula to model the probability of interaction between ocupant and system."@en ;
                    rdfs:label "Interaction Formula"@en .


###  http://www.semanticweb.org/bimmer/ob#Light
:Light rdf:type owl:Class ;
       rdfs:subClassOf :System ;
       rdfs:label "Light"@en .


###  http://www.semanticweb.org/bimmer/ob#Linear1DFormula
:Linear1DFormula rdf:type owl:Class ;
                 rdfs:subClassOf :InteractionFormula ;
                 rdfs:comment "Class to represent mathematical formulas of the type Linear 1D."@en ;
                 rdfs:label "Linear 1D Formula"@en .


###  http://www.semanticweb.org/bimmer/ob#Linear2DFormula
:Linear2DFormula rdf:type owl:Class ;
                 rdfs:subClassOf :InteractionFormula ;
                 rdfs:comment "Class to represent mathematical formulas of the type Linear 2D."@en ;
                 rdfs:label "Linear 2D Formula"@en .


###  http://www.semanticweb.org/bimmer/ob#Linear3DFormula
:Linear3DFormula rdf:type owl:Class ;
                 rdfs:subClassOf :InteractionFormula ;
                 rdfs:comment "Class to represent mathematical formulas of the type Linear 3D."@en ;
                 rdfs:label "Linear 3D Formula"@en .


###  http://www.semanticweb.org/bimmer/ob#Logit2DLinearFormula
:Logit2DLinearFormula rdf:type owl:Class ;
                      rdfs:subClassOf :InteractionFormula ;
                      rdfs:comment "Class to represent mathematical formulas of the type Logit 2D Linear."@en ;
                      rdfs:label "Logit 2D Linear Formula"@en .


###  http://www.semanticweb.org/bimmer/ob#Logit3DLinearFormula
:Logit3DLinearFormula rdf:type owl:Class ;
                      rdfs:subClassOf :InteractionFormula ;
                      rdfs:comment "Class to represent mathematical formulas of the type Logit 3D Linear."@en ;
                      rdfs:label "Logit 3D Linear Formula"@en .


###  http://www.semanticweb.org/bimmer/ob#Logit4DLinearFormula
:Logit4DLinearFormula rdf:type owl:Class ;
                      rdfs:subClassOf :InteractionFormula ;
                      rdfs:comment "Class to represent mathematical formulas of the type Logit 4D Linear."@en ;
                      rdfs:label "Logit 4D Linear Formula"@en .


###  http://www.semanticweb.org/bimmer/ob#Meeting
:Meeting rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :programmedAt ;
